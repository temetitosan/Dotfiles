Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

URxvt*imLocale: en-US.UTF-8
URxvt*tername: rxvt-unicode-256color
URxvt*buffered: false
URxvt.intensifyStyles: true
URxvt.font: xft:Monospace:style=Medium:size=12
URxvt.boldFont: xft:Monospace:style=Bold:size=12
URxvt.scrollBar: false

XTerm.vt100.faceName: Monospace:size=12:antialias=true
XTerm.vt100.font: 7x13
XTerm.vt100.internalBorder: 2
XTerm.vt100.saveLines: 4096
XTerm.vt100.selectToClipboard: true
XTerm.vt100.scrollBar: false
XTerm*metaSendsEscape: true

